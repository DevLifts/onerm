  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ]
  },
  "dependencies": {
    "expo": "^27.0.1",
    "prop-types": "^15.6.2",
    "react": "16.3.1",
    "react-native": "~0.55.2"
  }
}
